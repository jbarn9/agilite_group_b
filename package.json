{
  "name": "aelion",
  "version": "0.0.1",
  "description": "Exploitation d'une API REST en JS",
  "main": "src/main.js",
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "api:start": ".\\node_modules\\.bin\\json-server datas\\db.json",
    "start": "npm-run-all --parallel start:api start:frontend",
    "start:api": "json-server --watch db.json --port 3001",
    "start:frontend": "http-server src -p 3000 -c-1 -o",
    "dev": "npm start",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run"
  },
  "keywords": [
    "JS",
    "API",
    "REST"
  ],
  "author": "LePaulM",
  "license": "UNLICENSED",
  "devDependencies": {
    "cypress": "^15.10.0",
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "json-server": "^1.0.0-beta.3"
  },
  "dependencies": {
    "ag-grid-community": "^35.0.1"
  }
}
